{
  "name": "dial-scan",
  "description": "Программа для анализа DialPlan VoIp-сервера Asterisk",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/orion55/dial-scan.git"
  },
  "keywords": [
    "анализ",
    "DialPlan",
    "Asterisk"
  ],
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "npx tsx src/index.ts",
    "copy-settings": "cpx \"src/settings.json\" dist/",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "run": "ncc run src/index.ts",
    "build": "npm run clean && ncc build src/index.ts -o dist -m && npm run copy-settings"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/ansi-colors": "^3.2.6",
    "@types/node": "^20.17.47",
    "@vercel/ncc": "^0.38.3",
    "cpx": "^1.2.1",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "3.5.3",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.32.1"
  },
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "cfonts": "^3.3.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.5.0",
    "ini": "^5.0.0",
    "winston": "^3.17.0"
  }
}
